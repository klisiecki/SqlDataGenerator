<?xml version="1.0"?>
<TABLES xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="pl.poznan.put.sqldatagenerator schema.xsd"
        xmlns="pl.poznan.put.sqldatagenerator">
    <TABLE>
        <NAME>PRODUCTS</NAME>
        <ROWS_NUM>300</ROWS_NUM>
        <ATTRIBUTES>
            <ATTRIBUTE>
                <NAME>ID</NAME>
                <TYPE>INTEGER</TYPE>
                <PRIMARY_KEY>true</PRIMARY_KEY>
            </ATTRIBUTE>

            <ATTRIBUTE>
                <NAME>NAME</NAME>
                <TYPE>VARCHAR</TYPE>
                <MIN_VALUE>3</MIN_VALUE>
                <MAX_VALUE>10</MAX_VALUE>
            </ATTRIBUTE>

            <ATTRIBUTE>
                <NAME>DESCRIPTION</NAME>
                <TYPE>VARCHAR</TYPE>
                <MIN_VALUE>1</MIN_VALUE>
                <MAX_VALUE>50</MAX_VALUE>
            </ATTRIBUTE>

            <ATTRIBUTE>
                <NAME>PRICE</NAME>
                <TYPE>FLOAT</TYPE>
                <MIN_VALUE>0</MIN_VALUE>
                <MAX_VALUE>100000</MAX_VALUE>
            </ATTRIBUTE>

            <ATTRIBUTE>
                <NAME>OLD_PRICE</NAME>
                <TYPE>FLOAT</TYPE>
                <MIN_VALUE>0</MIN_VALUE>
                <MAX_VALUE>100000</MAX_VALUE>
            </ATTRIBUTE>

            <ATTRIBUTE>
                <NAME>CATEGORY</NAME>
                <TYPE>VARCHAR</TYPE>
                <VALUES>
                    <VALUE>AGD</VALUE>
                    <VALUE>RTV</VALUE>
                    <VALUE>COMPUTERS</VALUE>
                    <VALUE>ELECTRONICS</VALUE>
                </VALUES>
            </ATTRIBUTE>

            <ATTRIBUTE>
                <NAME>PACKAGE_WIDTH</NAME>
                <TYPE>INTEGER</TYPE>
                <MIN_VALUE>1</MIN_VALUE>
                <MAX_VALUE>5000</MAX_VALUE>
            </ATTRIBUTE>

            <ATTRIBUTE>
                <NAME>PACKAGE_HEIGHT</NAME>
                <TYPE>INTEGER</TYPE>
                <MIN_VALUE>1</MIN_VALUE>
                <MAX_VALUE>5000</MAX_VALUE>
            </ATTRIBUTE>

            <ATTRIBUTE>
                <NAME>PACKAGE_DEPTH</NAME>
                <TYPE>INTEGER</TYPE>
                <MIN_VALUE>1</MIN_VALUE>
                <MAX_VALUE>5000</MAX_VALUE>
            </ATTRIBUTE>

        </ATTRIBUTES>
    </TABLE>

    <TABLE>
        <NAME>CLIENTS</NAME>
        <ROWS_NUM>100</ROWS_NUM>
        <ATTRIBUTES>
            <ATTRIBUTE>
                <NAME>ID</NAME>
                <TYPE>INTEGER</TYPE>
                <PRIMARY_KEY>true</PRIMARY_KEY>
            </ATTRIBUTE>

            <ATTRIBUTE>
                <NAME>FIRST_NAME</NAME>
                <TYPE>VARCHAR</TYPE>
                <MIN_VALUE>2</MIN_VALUE>
                <MAX_VALUE>15</MAX_VALUE>
            </ATTRIBUTE>

            <ATTRIBUTE>
                <NAME>LAST_NAME</NAME>
                <TYPE>VARCHAR</TYPE>
                <MIN_VALUE>2</MIN_VALUE>
                <MAX_VALUE>15</MAX_VALUE>
            </ATTRIBUTE>

            <ATTRIBUTE>
                <NAME>BIRTH_DATE</NAME>
                <TYPE>TIMESTAMP</TYPE>
            </ATTRIBUTE>

        </ATTRIBUTES>
    </TABLE>

    <TABLE>
        <NAME>ORDERS</NAME>
        <ROWS_NUM>1000</ROWS_NUM>
        <ATTRIBUTES>
            <ATTRIBUTE>
                <NAME>ID</NAME>
                <TYPE>INTEGER</TYPE>
                <PRIMARY_KEY>true</PRIMARY_KEY>
            </ATTRIBUTE>

            <ATTRIBUTE>
                <NAME>PRODUCT_ID</NAME>
                <TYPE>INTEGER</TYPE>
            </ATTRIBUTE>

            <ATTRIBUTE>
                <NAME>CLIENT_ID</NAME>
                <TYPE>INTEGER</TYPE>
            </ATTRIBUTE>

            <ATTRIBUTE>
                <NAME>DATETIME</NAME>
                <TYPE>TIMESTAMP</TYPE>
            </ATTRIBUTE>

            <ATTRIBUTE>
                <NAME>STATE</NAME>
                <TYPE>INTEGER</TYPE>
                <VALUES>
                    <VALUE>0</VALUE>
                    <VALUE>1</VALUE>
                    <VALUE>2</VALUE>
                </VALUES>
            </ATTRIBUTE>
        </ATTRIBUTES>
    </TABLE>
</TABLES>
