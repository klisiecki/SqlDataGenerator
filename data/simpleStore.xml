<?xml version="1.0"?>
<TABLES xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="pl.poznan.put.sqldatagenerator schema.xsd"
        xmlns="pl.poznan.put.sqldatagenerator" M="200" T="100">
    <TABLE>
        <NAME>PRODUCTS</NAME>
        <ROWS_NUM>50</ROWS_NUM>
        <ATTRIBUTES>
            <ATTRIBUTE>
                <NAME>ID</NAME>
                <TYPE>INTEGER</TYPE>
                <PRIMARY_KEY>true</PRIMARY_KEY>
            </ATTRIBUTE>

            <ATTRIBUTE>
                <NAME>PRICE</NAME>
                <TYPE>FLOAT</TYPE>
                <MIN_VALUE>1</MIN_VALUE>
                <MAX_VALUE>10000</MAX_VALUE>
            </ATTRIBUTE>

            <ATTRIBUTE>
                <NAME>CATEGORY</NAME>
                <TYPE>INTEGER</TYPE>
                <MIN_VALUE>1</MIN_VALUE>
                <MAX_VALUE>5</MAX_VALUE>
            </ATTRIBUTE>

        </ATTRIBUTES>
    </TABLE>

    <TABLE>
        <NAME>CLIENTS</NAME>
        <ROWS_NUM>100</ROWS_NUM>
        <ATTRIBUTES>
            <ATTRIBUTE>
                <NAME>ID</NAME>
                <TYPE>INTEGER</TYPE>
                <PRIMARY_KEY>true</PRIMARY_KEY>
            </ATTRIBUTE>
            <ATTRIBUTE>
                <NAME>AGE</NAME>
                <TYPE>INTEGER</TYPE>
                <MIN_VALUE>10</MIN_VALUE>
                <MAX_VALUE>124</MAX_VALUE>
            </ATTRIBUTE>

        </ATTRIBUTES>
    </TABLE>

    <TABLE>
        <NAME>ORDERS</NAME>
        <ROWS_NUM>1000</ROWS_NUM>
        <ATTRIBUTES>
            <ATTRIBUTE>
                <NAME>ID</NAME>
                <TYPE>INTEGER</TYPE>
                <PRIMARY_KEY>true</PRIMARY_KEY>
            </ATTRIBUTE>

            <ATTRIBUTE>
                <NAME>PRODUCT_ID</NAME>
                <TYPE>INTEGER</TYPE>
            </ATTRIBUTE>

            <ATTRIBUTE>
                <NAME>CLIENT_ID</NAME>
                <TYPE>INTEGER</TYPE>
            </ATTRIBUTE>
        </ATTRIBUTES>
    </TABLE>
</TABLES>
